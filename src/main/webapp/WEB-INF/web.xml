<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <display-name>Medicare Web Application</display-name>

  <servlet>
    <servlet-name>patientServlet</servlet-name>
    <servlet-class>com.medicare.web.patient.patientServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>patientServlet</servlet-name>
    <url-pattern>/index</url-pattern>  <!-- Assure-toi que le mapping correspond à l'URL d'accès -->
  </servlet-mapping>
</web-app>
